<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="listBean" class="cn.javass.spring.chapter3.bean.ListTestBean">
        <property name="values">
            <list>
                <value>1</value>
                <value>2</value>
                <value>3</value>
            </list>
        </property>

        <property name="stringValues">
            <array>
                <value>s1</value>
                <value>s2</value>
                <value>s3</value>
                <value>s4</value>
            </array>
        </property>

        <property name="mpaValues">
            <map key-type="java.lang.String" value-type="java.lang.Integer">
                <entry key="K1" value="1"/>
                <entry key="K2" value="2"/>
                <entry key="K3" value="3"/>
                <entry key="K4" value="4"/>
                <entry key="K5" value="5"/>
            </map>
        </property>
    </bean>
</beans>